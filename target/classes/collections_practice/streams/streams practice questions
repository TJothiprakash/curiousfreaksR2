BigDecimal maxSalary = employees.stream()
                                .map(Employee::getSalary)
                                .max(Comparator.naturalOrder())
                                .get();

employees.stream()
         .filter(e -> e.getSalary().equals(maxSalary))
         .collect(Collectors.toList());
